{
  "name": "spacesbyhaley",
  "version": "1.0.0",
  "repository": "https://github.com/meredithmurfin/spacesbyhaley.git",
  "homepage": "https://spacesbyhaley.com",
  "license": "MIT",
  "author": {
    "name": "Meredith Murfin",
    "email": "meredithmurfin@gmail.com",
    "url": "http://meremurf.dev"
  },
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "clean": "rm -rf public && rm -rf .cache && gatsby clean",
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "develop": "yarn develop",
    "deploy": "yarn build && gh-pages -d public",
    "serve": "gatsby serve",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:styles": "stylelint src/**/*.css",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "test": "gatsby test",
    "type-check": "tsc",
    "eject": "gatsby eject"
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "antd": "^4.12.2",
    "gatsby": "^2.31.1",
    "gatsby-cli": "^2.19.0",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-stylelint": "^3.2.0",
    "gh-pages": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.6",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "babel-plugin-root-import": "6.6.0",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "write-good": "^1.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  }
}
